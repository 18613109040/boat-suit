.account-header-bg{
  background: #4283F4;
  height: 128rpx;
}
.account-info{
  display: flex;
  align-items: center;
  padding: 32rpx;
  background: #fff;
  margin: 32rpx;
  margin-top: -80rpx;
  border-radius: 6rpx;
}
.account-image{
  width: 128rpx;
  height: 128rpx;
  margin-right: 20rpx;
}
.account-name{
  font-family: PingFangSC-Semibold;
  font-size: 48rpx;
  color: #181C28;
  text-align: left;
}
.account-phone{
  margin-top: 12rpx;
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #7F8593;
}
.account-prv{
  display: flex;
  align-items: center;
  background: #fff;
  margin: 32rpx;
  padding: 32rpx;
  border-radius: 6rpx;
}
.image-icon{
  width: 44rpx;
  height: 44rpx;
  margin-right: 23rpx;
}
.account-nav{
  margin: 32rpx;
  padding: 0rpx 32rpx 0rpx 32rpx;
  background: #fff;
  border-radius: 6rpx;
}
.collection{
  display: flex;
  align-items: center;
  height: 104rpx;
}
.no-collect{
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #7F8593;
  margin-right: 30rpx;
}
.longin-text{
  font-family: PingFangSC-Semibold;
  font-size: 48rpx;
  color: #181C28;
  text-align: left;
}
.login-tag{
  background: #67CE70;
  border-radius: 28rpx;
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #FFFFFF;
  text-align: center;
  width: 152rpx;
  height: 48rpx;
  line-height: 48rpx;
  margin-left: 16rpx;
}