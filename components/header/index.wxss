.custom {
  position: fixed;
  width: 100%;
  top: 0;
  z-index: 99999999;
}
.nav-bar{
  display: flex;
  align-items: center;
  height: 45px;
  justify-content: space-between;
}
.nav-bar .action {
  display: flex;
  align-items: center;
  height: 100%;
  justify-content: center;
  max-width: 100%;
  font-size: 30rpx;
  color: #323232;
  
}
.nav-bar .title{
  flex: 1;
  text-align: center;
  margin-right: 150rpx;
  font-family: PingFangSC-Medium;
  font-size: 30rpx;
  color: #323232;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.bg-gradual-blue {
  background-image: linear-gradient(45deg, #0081ff, #1cbbb4);
}